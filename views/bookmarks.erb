<ul>
  <% @bookmarks.each do |bookmark| %>
    <li class="bookmark" id="bookmark-<%=bookmark.id%>"> 
      <a href=<%= bookmark.url %>> <%= bookmark.title %> </a> 
      <form action="/bookmarks/<%= bookmark.id%>" method="post">
        <input type="submit" name='_method' value="Delete"/>
      </form>
    </li>
  <% end %>
</ul>
